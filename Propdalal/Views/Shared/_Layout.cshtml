<!DOCTYPE html>
<html lang="en-us">

<head>
    <title>Prop Dalal</title>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <meta charset="UTF-8">
    <meta name="author" content="jsjhass">
    <meta name="keywords" content="Propdalal" />
    <meta name="description" content="Property sale purchase rent" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="content-type" content="text/html; charset=UTF-8" />
    <link rel="stylesheet" href="~/css/bootstrap.min.css" type="text/css" />
    <link rel="stylesheet" href="~/css/bootstrap-theme.min.css" type="text/css" />
    <link rel="stylesheet" href="~/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/css/flexslider.css">
    <link rel="stylesheet" href="~/css/select-theme-default.css">
    <link rel="stylesheet" href="~/css/owl.carousel.css">
    <link rel="stylesheet" href="~/css/owl.theme.css">
    <link rel="stylesheet" href="~/css/jquery.fancybox.css" />
    <link rel="stylesheet" href="~/css/style.css" type="text/css" />
    <!--[if IE]>
            <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
</head>

<body id='top'>
    <header></header>
    <!-- /Header -->
    <div class="slider-section">
        <div id="premium-bar">
            <div class="container">
                <nav class="navbar navbar-default" role="navigation">
                    <div class="container-fluid">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                           
                                <a class="navbar-brand" href="/home/index"><img src="~/Content/images/logo.png" alt="logo"></a>
                            
                        </div>
                        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                            <ul class="nav navbar-nav navbar-right">
                                <li>@Html.ActionLink("Home","Index","Home")</li>
                                <li>@Html.ActionLink("About", "About", "Home")</li>
                                <li>@Html.ActionLink("Rent", "Index", "Rentdetail")</li>
                                <li>@Html.ActionLink("Sale", "Index", "Saledetail")</li>
                                <li>@Html.ActionLink("News", "Index", "Blog")</li>
                                <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
   
                            </ul>
                        </div>
                        <!-- /.navbar-collapse -->
                    </div>
                    <!-- /.container-fluid -->
                </nav>
            </div>
        </div>
        <!-- Slider-Section -->
        @if (ViewBag.home == "Home")
        {
            <div class="main-flexslider">
                <ul class="slides">
                    <li class='slides' id='slide-n1'>
                        <img src="~/Content/images/slide-01.jpg" alt="slide 01">
                    </li>
                    <li class='slides' id='slide-n2'>
                        <img src="~/Content/images/slide-02.jpg" alt="slide 02">
                    </li>
                    <li class='slides' id='slide-n3'>
                        <img src="~/Content/images/slide-03.jpg" alt="slide 03">
                    </li>
                </ul>
            </div>
        }
        else
        {
            <div class="page-title-section" style="height: 107px;">
                <div class="container">
                    <div class="pull-left page-title">

                    </div>
                    
                </div>
            </div>

        }
    </div>
    <style>
        .submitdetl {
            height: 50px;
            width: 95px;
            position: fixed;
            top: 250px;
            z-index: 999999;
            right: 0px;
            color: #EBE8DF;
            background-color: #0095E6;
            text-align: center;
            padding: 10px;
            border-radius: 20px;
            font-weight: bold;
            /*font-size: 22px;*/
        }

            .submitdetl a {
                font-size: 12px;
                color: white;
            }
    </style>
    <div class="submitdetl">
        @Html.ActionLink("Submit Sale","Submitsale","Propertylist")
    </div>
    <!-- Search-Section -->
    @RenderBody()
    <div>

        <img src="~/Content/images/silhouette_buildings.png" style="width:100%" />
    </div>
    <footer style="color : #303133 !important;padding-bottom:0px !important">
        <div class="container">
            <div class="col-md-3 footer-about">
                <a class="logo" href="#"><img src="~/Content/images/logo.png" alt="logo" style=" Max-height: 50px; Max-width: 250px;"></a>
                <p>
                    Prop Dalal is a fastest growing marketplace for property buy and sell.
                </p>
            </div>
            <div class="col-md-3 footer-recent-posts">
                <h3 class='footer-title'>Address</h3>
                <ul>
                    <li><a href="#"><i class=" fa fa-arrow-circle-right"></i>Jaipur</a></li>

                    <li style="margin-top:10px !important"><a href="#" style="font-size:16px !important">Contact</a></li>
                    <li><a href="#"><i class="fa fa-arrow-circle-right"></i> 7777777777</a></li>
                </ul>
            </div>
            <div class="col-md-3 footer-contact-info">
                <h3 class='footer-title'>contact info</h3>
                <p class="website-number">
                    <i class="fa fa-phone"></i> +91 7777777777
                </p>
                <p class="website-number">
                    <i class="fa fa-phone"></i> +91 7777777777

                </p>
                <p class="website-fax">
                    <i class="fa fa-print"></i> 0141-777777
                </p>
            </div>
            <div class="col-md-3 footer-contact-info">
                <h3 class='footer-title'>mail info</h3>

                <p class="website-email">
                    <i class="fa fa-envelope"></i> info@domain.com
                </p>
                <p class="website-email">
                    <i class="fa fa-envelope"></i> info@domain.com
                </p>
                <p class="website-email">
                    <i class="fa fa-envelope"></i> info@domain.com
                </p>

            </div>
            @*<div class="col-md-3 footer-newsletter">
                    <h3 class='footer-title'>newsletter</h3>
                    <p>
                        Subscrib For News Letter
                    </p>
                    <form class='footer-search'>
                        <input type="text" placeholder='e-mail'>
                        <input type="submit">
                    </form>


                </div>*@

            <div class="col-md-12 footer-about" style="text-align:right">

                <p style="text-align:right;font-weight:800" class='footer-title'>  Developed by&nbsp;jsjhass</p>
            </div>
        </div>

    </footer>

    <!-- Javascript -->
    <script type="text/javascript" src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="~/Scripts/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.flexslider-min.js"></script>
    <script type="text/javascript" src="~/Scripts/select.min.js"></script>
    <script type="text/javascript" src="~/Scripts/owl.carousel.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.fancybox.js"></script>
    <script type="text/javascript" src="~/Scripts/script.js"></script>
    @RenderSection("scripts", required: false)
</body>

</html>